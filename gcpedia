	$(document).ready(function(){
		$(".planningMap").hide();
		$("#btnli").click(function(){
			$(".planningMap").show();
			$('.item').not(".planningMap").hide();
		});
	});

	$(document).ready(function(){
		$(".PLARMap").hide();
		$("#btnplar").click(function(){
			$(".PLARMap").show();
			$('.item').not(".PLARMap").hide();
		});
	});

	$(document).ready(function(){
		$(".EOIMap").hide();
		$("#btneoi").click(function(){
			$(".EOIMap").show();
			$('.item').not(".EOIMap").hide();
		});
	});

	$(document).ready(function(){
		$(".changeofoMap").hide();
		$("#btncoo").click(function(){
			$(".changeofoMap").show();
			$('.item').not(".changeofoMap").hide();
		});
	});

	$(document).ready(function(){
		$(".LOCMap").hide();
		$("#btnlo").click(function(){
			$(".LOCMap").show();
			$('.item').not(".LOCMap").hide();
		});
	});

