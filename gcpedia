	<script>


	/* Map Category Selector */
	$('#all').click(function(){
			$('.item').show();
			$('.planningMap').hide();
			$('.PLARMap').hide();
			$('.EOIMap').hide();
			$('.LOCMap').hide();
			$('.parkingMap').hide();
			$('.changeofoMap').hide();
			$('.greeningMap').hide();
			$('.actingPolicy').hide();
			$('.advertisingPolicy').hide();
			$('.p3Policy').hide();
			$('.terminationPolicy').hide();
			$('.escalationPolicy').hide();
			$('.estoppelPolicy').hide();
			$('.foodPolicy').hide();
			$('.escadminPolicy').hide();
			$('.awardPolicy').hide();
			$('.riskPolicy').hide();
			$('.RP1Policy').hide();
			$('.thirdPolicy').hide();
			$('.daycarePolicy').hide();
			$('.estimationPolicy').hide();
			return false;
	});

	$('#lease-in').click(function(){
			$('.item').show();
			$('.item').not('.lease-in').hide();
			$('.planningMap').hide();
			$('.negotiationMap').hide();
			return false;
	});

